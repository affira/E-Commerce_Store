{
  "name": "software-testing-assignment",
  "version": "1.0.0",
  "description": "Software Testing Assignment for utility functions",
  "main": "index.js",
  "type": "module",
  "scripts": {
  "test": "node --experimental-modules node_modules/.bin/jest --config jest.config.js",
  "test:watch": "node --experimental-modules node_modules/.bin/jest --watch --config jest.config.js",
  "test:coverage": "node --experimental-modules node_modules/.bin/jest --coverage --config jest.config.js",
  "coverage": "node --experimental-modules node_modules/.bin/jest --coverage --config jest.config.js && coveralls < coverage/lcov.info",
  "lint": "eslint src test"
},
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "babel-jest": "^29.7.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.54.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0"
  }
}